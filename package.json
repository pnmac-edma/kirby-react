    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "aws4-browser": "^1.0.0",
    "closest": "0.0.1",
    "date-fns": "^2.0.0-beta.5",
    "dompurify": "^2.0.8",
    "formik": "^2.1.4",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "mathjs": "^6.2.5",
