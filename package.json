    "react-window": "^1.8.5",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tokens": "rm -rf package-lock.json && npm i @edma/design-tokens --save-dev",
    "release": "release-it",
    "tsc": "tsc"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@edma/design-tokens": "^4.2.8",
    "axios": "^0.19.0",
    "bestzip": "^2.1.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "release-it": "^12.4.3",
    "rimraf": "^3.0.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@edma/design-tokens|react-router-transition)"
    ]
  }
}
